<h1>Specific librarian request</h1>

<p id="notice"><%= notice %></p>

<table>
  <thead>
  <tr>
    <th>Email</th>
    <th>Name</th>
    <th>Password</th>
    <th colspan="3"></th>
  </tr>
  </thead>

  <tbody>
  <% @lib.each do |librarian| %>
    <tr>
      <td><%= librarian.email %></td>
      <td><%= librarian.name %></td>
      <td><%= librarian.password %></td>
      <td><%= link_to 'Approve', approve_librarian_path(librarian) %></td>
      <td><%= link_to 'Edit', edit_admin_path(librarian) %></td>
      <!--        <td><%#= link_to 'Approve', librarian_requests_path(admin) %></td>-->
      <td><%= link_to 'Destroy', librarian, method: :delete, data: { confirm: 'Are you sure?' } %></td>
    </tr>
  <% end %>
  </tbody>
</table>

<br>
